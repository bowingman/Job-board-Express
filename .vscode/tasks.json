{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "label": "tsc:dev",
            "type": "shell",
            "command": "${workspaceFolder}/node_modules/typescript/bin/tsc --sourceMap --removeComments false --pretty --noUnusedLocals false --noUnusedParameters false --project ./tsconfig.json",
            "group": "build"
        }
    ]
}
